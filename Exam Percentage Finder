Console.WriteLine("-------->EXAMINATION MARKS TESTOR<--------");
Console.WriteLine("       ------->BY BIOZFROG<-------\n");

Console.WriteLine("\nEnter 1st Subject");
string s1 = Console.ReadLine();

Console.WriteLine("\nEnter 2nd Subject");
string s2 = Console.ReadLine();

Console.WriteLine("\nEnter 3rd Subject");
string s3 = Console.ReadLine();

Console.WriteLine("Enter 4th Subject");
string s4 = Console.ReadLine();

Console.WriteLine("\nEnter 5th Subject");
string s5 = Console.ReadLine();

Console.WriteLine("\nEnter 6th Subject");
string s6 = Console.ReadLine();

Console.WriteLine("\nEnter 7th Subject");
string s7 = Console.ReadLine();

Console.WriteLine("\nEnter 8th Subject");
string s8 = Console.ReadLine();

Console.WriteLine($"\nEnter your {s1} Marks Obtained: ");
double Physics = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter Total Marks of {s1}: ");
double TotalPhysics = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter your {s2} Marks Obtained: ");
double Mathematics = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter Total Marks of {s2}: ");
double TotalMathematics = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter your {s3} Marks Obtained: ");
double Urdu = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter Total Marks of {s3}: ");
double TotalUrdu = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter your {s4} Marks Obtained: ");
double Chemistry = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter Total Marks of {s4}: ");
double TotalChemistry = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter your {s5} Marks Obtained: ");
double English = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter  Total Marks of {s5}: ");
double TotalEnglish = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter your {s6} Marks Obtained: ");
double PakStudies = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter  Total Marks of {s6}: ");
double TotalPakStudies = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter your {s7} Marks Obtained: ");
double Islamiat = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter  Total Marks of {s7}: ");
double TotalIslamiat = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter your {s8} Marks Obtained: ");
double ICT = Convert.ToDouble(Console.ReadLine());

Console.WriteLine($"\nEnter  Total Marks of {s8}: ");
double TotalICT = Convert.ToDouble(Console.ReadLine());

double totalMarks = TotalPhysics + TotalMathematics + TotalUrdu + TotalChemistry + TotalEnglish + TotalPakStudies + TotalIslamiat + TotalICT;
double Percent = (Physics + Mathematics + Urdu + Chemistry + English + PakStudies + Islamiat + ICT) / totalMarks * 100;

Console.Write("\nYou totally got {0:F1}%", Percent);

if(Percent >= 70)
{
    Console.Write(". You Passed = >\n");
}
else
{
    Console.Write(". You Failed =<\n");
}
